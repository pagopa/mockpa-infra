<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

	<changeSet author="liquibase" id="202306191615">
		<preConditions onFail="MARK_RAN">
			<foreignKeyConstraintExists
				foreignKeyName="fk_ica_binary_file" />
		</preConditions>
		<dropForeignKeyConstraint
			baseTableName="iban_master" constraintName="fk_ica_binary_file" />
	</changeSet>
	
	<changeSet author="liquibase" id="202306191617">
		<addColumn tableName="ica_binary_file">
			<column name="id_dominio" type="VARCHAR(35)"
				afterColumn="obj_id">
				<constraints nullable="false" />
			</column>
		</addColumn>
	</changeSet>
</databaseChangeLog>
