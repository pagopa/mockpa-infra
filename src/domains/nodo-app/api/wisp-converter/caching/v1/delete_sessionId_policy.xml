<policies>
  <inbound>
    <base />
    <set-variable name="wisp_delete_sessionId" value="@((JArray)context.Request.Body.As<JObject>()["sessionId"])" />
    <cache-remove-value key="@("wisp_timer_hang_" + (string)context.Variables["wisp_delete_sessionId"])" caching-type="external" />
  </inbound>
  <outbound>
    <base />
  </outbound>
  <backend>
    <base />
  </backend>
  <on-error>
    <base />
  </on-error>
</policies>
